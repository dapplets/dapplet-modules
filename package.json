{
  "private": true,
  "name": "dapplet-modules",
  "version": "0.1.0",
  "description": "Examples of modules for Dapplet Extension",
  "author": "Dapplets Team",
  "license": "MIT",
  "keywords": [
    "dapplets",
    "ethereum"
  ],
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "npm-run-all --parallel serve watch",
    "build": "rollup --config rollup.config.js",
    "watch": "rollup -w --config rollup.config.js",
    "serve": "node src/server/index.js",
    "deploy": "lerna exec --concurrency 1 -- dapplet-cli deploy",
    "deploy:archive": "lerna exec --concurrency 1 -- dapplet-cli deploy --archive",
    "deployswarm": "lerna exec --concurrency 1 -- dapplet-cli deploy --swarm",
    "deployswarm:archive": "lerna exec --concurrency 1 -- dapplet-cli deploy --swarm --archive",
    "create": "node src/copyTemplate.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dapplets/typescript-examples.git"
  },
  "bugs": {
    "url": "https://github.com/dapplets/typescript-examples/issues"
  },
  "homepage": "https://github.com/dapplets/typescript-examples#readme",
  "devDependencies": {
    "@dapplets/dapplet-extension": "^0.1.5",
    "@rollup/plugin-image": "^2.0.1",
    "@rollup/plugin-typescript": "^3.0.0",
    "chalk": "^3.0.0",
    "ejs": "^3.0.1",
    "http-server": "^0.12.1",
    "inquirer": "^7.0.3",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.30.1",
    "shelljs": "^0.8.3",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "https": "^1.0.0"
  }
}
